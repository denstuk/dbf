{
  "name": "dbfarm",
  "version": "0.0.3",
  "description": "DB Farm",
  "main": "./bin/index.js",
  "bin": {
    "dbfarm": "./bin/index.js"
  },
  "scripts": {
    "build": "rm -rf ./bin && tsc -p ./tsconfig.json",
    "start": "node ./bin/index.js",
    "start:docker": "cd docker && docker-compose up --build --force-recreate",
    "lint:eslint": "eslint . --fix",
    "make": "npm run build && npm install -g ."
  },
  "author": "Denis Stuk <denstuk@outlook.com>",
  "license": "MIT",
  "dependencies": {
    "@databases/pg": "^5.5.0",
    "@faker-js/faker": "^8.4.1",
    "chalk": "^4.1.2",
    "pg": "^8.11.3",
    "yargs": "^17.7.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/pg": "^8.11.3",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "typescript": "^5.4.2"
  }
}
